default vesamenu.c32
prompt 0
timeout 600
ONTIMEOUT local

MENU TITLE PXE Menu

LABEL local
  MENU LABEL Boot local hard drive
  LOCALBOOT 0

LABEL Install Cisco NFV Infrastructure Software
  MENU LABEL Install Cisco NFV Infrastructure Software
  KERNEL os/images/pxeboot/vmlinuz
  APPEND initrd=os/images/pxeboot/initrd.img method=nfs:{{ ansible_host }}:/pxeroot/os ks=nfs:{{ ansible_host }}:/pxeroot/os/ks.cfg vbranch_nfv